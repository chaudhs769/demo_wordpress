{
  "version": 2,
  "builds": [
      { "src": "wp-config.php", "use": "@juicyfx/php@0.0.6", "config": {"mode": "fpm"} }
    ],
  
  "routes": [
    { "src": "/wp-admin/?", "dest": "index.php" },
    { "src": ".*\\.php$", "dest": "index.php" },
    { "src": "//*.@(eot|otf|ttf|ttc|woff|font.css|css|js|ico|png|jpg|jpeg)", "headers": {"cache-control": "s-maxage=604800"} }
  ],
  "regions": ["icn1"]
}
